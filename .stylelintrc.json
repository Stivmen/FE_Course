{
  "extends": "stylelint-config-standard",
  "rules": {
    "block-no-empty": true,
    "indentation": 4,
    "no-eol-whitespace": [
        true,
        {
            "severity": "warning"
        }
    ],
    "max-line-length": [
        120,
        {
            "severity": "warning"
        }
    ],
    "no-extra-semicolons": true
  }
}
